  "commandDescriptions": {
    "add": {
      "description": "Füge einen RSS Feed in dem Channel wo der Befehl ausgeführt wird hinzu. Um mehrere Feedas können hinzuzufügen, trenne diese mit `>`.",
      "args": {
        "<link>": "Feed URL."
      }
    },
    "alert": {
      "description": "Füge oder entferne Nutzer, die eine Direktnachricht bekommen, wenn ein Feed eine Warnung oder einen Fehler schmeißt.",
      "args": {
        "add <user id/mention>": "Füge einen Nutzer hinzu, der Direktnachrichten erhalten soll.",
        "remove <user id/mention>": "Entferne einen Nutzer, der keine Direktnachrichten mehr erhalten soll.",
        "list": "Gib die Liste der aktuellen Nutzer aus, die Direktnachrichten erhalten."
      }
    },
    "backup": {
      "description": "Erstelle ein Backup vom Server, welches dir als JSON Anhang ausgegeben wird."
    },
    "clone": {
      "action": "Feed Einstellungen klonen",
      "description": "Klone Feed Einstellungen zu anderen Feeds"
    },
    "compare": {
      "action": "Setze Vergleiche zu Artikel Checks",
      "description": "Setze zusätzliche postive/negative Checks für Artikel für eine bessere Kontrolle welche Artikel auf deinen Server gesendet werden.",
      "args": {
        "list": "Gib die Liste aller aktuellen Vergleiche aus.",
        "(+|-)<property>": "Die positiven oder negativen Artikel Einstellungen zum hinzufügen für Vergleiche. Mehr als eine kann hinzugefügt werden."
      }
    },
    "date": {
      "description": "Öffnet ein Menü zum einstellen, wie das Datum angezeigt wird."
    },
    "dump": {
      "action": "Rohe Platzhalter Ausgabe",
      "args": {
        "original": "Gibt die originale, unformatierte JSON aus."
      }
    },
    "embed": {
      "action": "Embed einstellen",
      "description": "Öffnet ein Menü um eine Embed eines Feeds zu verändern. Dies ersetzt die normale Emved von Discord, welche normalerweise gesendet wird, wenn ein Link gesendet wird."
    },
    "embed.fields": {
      "action": "Embed Felder einstellen",
      "description": "Öffnet ein Menü um Embed Felder der Embed Nachricht eines Feeds zu verändern. Dies ersetzt die normale Emved von Discord, welche normalerweise gesendet wird, wenn ein Link gesendet wird."
    },
    "filters": {
      "action": "Feed Filter erstellen/entfernen",
      "description": "Öffnet ein Menü, um globale Filter für einen Feed zu erstellen oder zu entfernen. Nachrichten, die keine der angegebenen Wörter beinhalten, werden nicht auf deinen Discord gesendet."
    },
    "invite": {
      "description": "Gibt den Invite-Link für den Bot aus."
    },
    "list": {
      "description": "Gibt alle aktiven Feeds eines Servers aus.",
      "args": {
        "[<channel mention>] [<search query>]": "Markiere einen Channel, um nur Feeds in einem Channel zu anzuzeigen, und/oder um eine Suchanfrage hinzuzufügen."
      }
    },
    "locale": {
      "description": "Ändert die Sprache der Befehle (Wenn die gewünschte Sprache verfügbar ist)",
      "args": {
        "<locale>": "Die gewünschte Sprache"
      }
    },
    "mention": {
      "action": "Abonnenten einstellen",
      "description": "Öffnet ein Menü, um globale/gefilterte Abonnenten für Rollen/Nutzer von Feeds einzustellen."
    },
    "mention.filters": {
      "description": "Öffnet ein Menü, um Filter für Abonnenten zu verwalten, damit diese nur markiert werden, wenn der Filter zutrifft."
    },
    "move": {
      "description": "Öffnet ein Menü, um einen Feed in einen anderen Channel zu verschieben.",
      "action": "Feed Kanal verschieben"
    },
    "options": {
      "description": "Öffnet ein Menü für verschiedene Feed Einstellungen."
